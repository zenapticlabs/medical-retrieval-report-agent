ENVIRONMENT=development
DEBUG=true
# OpenSearch Configuration
USE_OPENSEARCH=false
OPENSEARCH_ENDPOINT=https://your-opensearch-endpoint.region.aoss.amazonaws.com
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-2
OPENSEARCH_INDEX_NAME=medical-documents
VECTOR_DIMENSION=384

# Elasticsearch Configuration (as fallback)
ELASTICSEARCH_HOST=elasticsearch
ELASTICSEARCH_PORT=9200
MAX_RETRIES=10
RETRY_INTERVAL=10


DOCUMENTS_DIR=Redacted
# Model settings
TRANSFORMERS_CACHE=/root/.cache

# MySQL settings
MYSQL_ROOT_PASSWORD=rootpassword123
MYSQL_DATABASE=medical_db
MYSQL_USER=medical_user
MYSQL_PASSWORD=medicalpass123
MYSQL_HOST=mysql
MYSQL_PORT=3306


JWT_SECRET_KEY=your-secret-key-here-change-this-in-production
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30


SHAREPOINT_TENANT_ID=your_tenant_id
SHAREPOINT_CLIENT_ID=your_client_id
SHAREPOINT_CLIENT_SECRET=your_client_secret
SHAREPOINT_SITE_ID=your_site_id